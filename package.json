    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "electron": "^13.2.2",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "prettier": "^2.3.2",
    "sass": "^1.26.5",
    "sass-loader": "^12.1.0",
    "typescript": "^4.3.5",
    "vue-cli-plugin-windicss": "~1.0.4"
  }
